  "scripts": {
    "build": "tsc -b && vite build",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "dev": "vite",
    "lint": "eslint . && stylelint \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --fix \"**/*.{css,less,scss}\"",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{css,less,scss}": "stylelint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@shadow-bridge/react": "^0.2.1",
    "antd": "^5.24.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.24.0",
    "eslint-config-ali": "^16.1.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-config-ali": "^1.3.2",
    "stylelint": "^16.18.0",
    "stylelint-config-ali": "^2.1.2",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.0"
  }
